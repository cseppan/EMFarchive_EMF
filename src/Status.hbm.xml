<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping> 
  <class name="gov.epa.emissions.framework.commons.Status" table="statusmessages">
 
    <id column="status_id" name="statusid" type="java.lang.Long"> 
      <generator class="sequence"/> 
    </id> 
    
	<property column="user_name" length="15" name="userName" not-null="true" type="java.lang.String"/>
	<property column="msg_type" length="255" name="msgType" not-null="true" type="java.lang.String"/>
	<property column="message" length="255" name="message" type="java.lang.String"/>
	<property column="date" length="10" name="timestamp" type="java.util.Date"/>
 
  </class> 
</hibernate-mapping>  

