# Properties for Deployment to Tomcat (compatible w/ version 4.1.31)

# app
app.name=emf
app.version=0.1
  
# basic settings - use '/' as a separator
jdk.home=<full path to JDK home - use '/' as a separator>
tomcat.home=<full path to Tomcat home i.e. installation directory >

# Tomcat Manager
tomcat.server=localhost
tomcat.manager.username=admin
tomcat.manager.password=admin
